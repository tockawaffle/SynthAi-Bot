{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["es2021"],
        "module": "commonjs",
        "rootDir": "./",
        "resolveJsonModule": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "exclude": ["dist"],
}