{
    "name": "synth-bot",
    "version": "0.8.1",
    "description": "A Discord bot that uses many different AI to generate text and images.",
    "main": "dist/src/bot.js",
    "scripts": {
        "dev": "ts-node --project ./tsconfig.json --files src/bot.ts",
        "build": "tsc",
        "watch": "tsc -w",
        "start": "node dist/src/bot.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tockawaffle/SynthAi-Bot.git"
    },
    "keywords": [
        "Discord",
        "ChatGPT",
        "OpenAI",
        "AI",
        "IA",
        "Discord",
        "Bot",
        "Bot"
    ],
    "author": "tockawaffle",
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/tockawaffle/SynthAi-Bot/issues"
    },
    "homepage": "https://github.com/tockawaffle/SynthAi-Bot#readme",
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@tockawa/openai-plugin": "^1.0.1",
        "@tockawa/wokcommands": "^2.1.4",
        "axios": "^1.4.0",
        "cheerio": "^1.0.0-rc.12",
        "discord.js": "^14.11.0",
        "dotenv": "^16.0.3",
        "gpt-3-encoder": "^1.1.4",
        "jspteroapi": "^1.11.1",
        "moment": "^2.29.4",
        "openai": "^3.2.1",
        "topgg-autoposter": "^2.0.1",
        "ytdl-core": "^4.11.4"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/node": "^20.1.0",
        "node_characterai": "^1.1.3",
        "typescript": "^5.0.4"
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules",
            "dist",
            "src/assets/"
        ],
        "watch": [
            "src",
            "types",
            "modules",
            "package.json"
        ],
        "ext": "ts,json",
        "exec": "npm run dev",
        "execMap": {
            "ts": "ts-node"
        },
        "verbose": true,
        "execOptions": {
            "env": {
                "NODE_ENV": "development"
            }
        }
    }
}
